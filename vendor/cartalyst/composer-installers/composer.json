{
	"name": "cartalyst/composer-installers",
	"type": "composer-plugin",
	"description": "Composer installers plugin for various Cartalyst packages.",
	"keywords": ["platform", "ecommerce", "laravel", "bootstrap"],
	"license": "proprietary",
	"authors": [
		{
			"name": "Cartalyst LLC",
			"email": "help@cartalyst.com"
		},
		{
			"name": "Christopher Pitt",
			"email": "cgpitt@gmail.com",
			"role": "Developer"
		},
		{
			"name": "Ben Corlett",
			"email": "ben.corlett@cartalyst.com",
			"role": "Developer"
		},
		{
			"name": "Bruno Gaspar",
			"email": "bruno.gaspar@cartalyst.com",
			"role": "Developer"
		},
		{
			"name": "Dan Syme",
			"email": "dan.syme@cartalyst.com",
			"role": "Creator & Designer"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"composer-plugin-api": "1.0.0"
	},
	"require-dev": {
		"mockery/mockery": "0.7.2"
	},
	"autoload": {
		"psr-4": {
			"Cartalyst\\ComposerInstallers\\": "src/"
		}
	},
	"extra": {
		"component": "package",
		"class": [
			"Cartalyst\\ComposerInstallers\\ExtensionInstallerPlugin",
			"Cartalyst\\ComposerInstallers\\InstallerPlugin",
			"Cartalyst\\ComposerInstallers\\ThemeInstallerPlugin"
		],
		"branch-alias": {
			"dev-master": "1.2-dev"
		}
	}
}
