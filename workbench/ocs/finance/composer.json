{
	"name": "ocs/finance",
	"type": "platform-extension",
	"description": "Finance",
	"authors": [
		{
			"name": "Mona Hashem",
			"email": "mhashem@overcoffeesolutions.com"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "http://packages.cartalyst.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"cartalyst/composer-installers": "1.2.*",
		"illuminate/support": "4.2.*",
		"platform/foundation": "1.0.*"
	},
	"autoload": {
		"classmap": [
			"database/migrations",
			"database/seeds"
		],
		"psr-4": {
			"Ocs\\Finance\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
